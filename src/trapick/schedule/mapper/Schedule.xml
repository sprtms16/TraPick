<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="trapick.schedule.mapper">
	<cache />
	<select id="selectCountry" resultType="Country">
		select * from country
	</select>
	<select id="selectCity" resultType="String" parameterType="int">
		select name from city where country_idx
	</select>
</mapper>















